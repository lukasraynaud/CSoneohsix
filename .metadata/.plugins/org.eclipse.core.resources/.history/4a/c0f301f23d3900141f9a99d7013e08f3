 */
public class ForceCalc {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
			
		double angle = 0.506145483;
		double gravity = 32;
		double d = 100;
		double v = Math.sqrt(d*gravity/Math.sin(angle)*2);
			
			System.out.println("You have to hit the ball" + v  );
			System.out.println("feet per second to hit the ball");
			System.out.println(d + "feet");
	}

}
