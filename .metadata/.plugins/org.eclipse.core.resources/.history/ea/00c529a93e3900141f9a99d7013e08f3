 */
public class ForceCalc {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
			
		double angle = 0.506145483;
		double gravity = 22;
		double d = 0.5;
		double v = Math.sqrt(d*gravity/Math.sin(angle)*2);
			
			System.out.println("You have to hit the ball  " + v  );
			System.out.println("miles per hour to hit the ball");
			System.out.println(d + "miles");
	}

}
