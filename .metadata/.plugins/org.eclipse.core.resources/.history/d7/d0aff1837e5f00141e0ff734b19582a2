import java.io.File;
import java.io.FileNotFoundException;
import java.util.Scanner;

public class Data {

	public static void main(String[] args) throws FileNotFoundException {
		// TODO Auto-generated method stub

		Scanner fileReader = new Scanner(new File("src/first150pokemon"));

		int i = 0;
		while (fileReader.hasNext()){ 
			
			
			System.out.println(fileReader.nextLine());
			i ++;
		} 
		
		double [] number = new double [i];
		String [] name = new String [i];
		double [] hp = new double [i];
		double [] attack = new double [i];
		double [] defense = new double [i];
		double [] sattack = new double [i];
		double [] sdefense = new double [i];
		double [] speed = new double [i];
		double [] total = new double [i];
		double [] average = new double [i];

		

		while (fileReader.hasNext()){

			number[i] = fileReader.nextDouble();
			name[i] = fileReader.next();
			hp[i] = fileReader.nextDouble();
			attack[i] = fileReader.nextDouble();
			defense[i] = fileReader.nextDouble();
			sattack[i] = fileReader.nextDouble();
			sdefense[i] = fileReader.nextDouble();
			speed[i] = fileReader.nextDouble();
			total [i] = fileReader.nextDouble();
			average[i] = fileReader.nextDouble();

			i++;

		}
		fileReader.close();

System.out.println(number[1]);
System.out.println(name[1]);



	}
}

